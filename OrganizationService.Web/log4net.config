<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="Master.Console" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%utcdate{yyyy-MM-dd HH:mm:ss.fff} %level %property{traceId} [%logger|%thread] %message%newline" />
    </layout>
  </appender>

  <root>
    <level value="TRACE" />
    <appender-ref ref="Master.Console" />
  </root>

</log4net>